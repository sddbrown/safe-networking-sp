 <log-settings>
      <http>
        <entry name="SFN HTTP Server">
          <server>
            <entry name="SFNapi">
              <address>54.173.131.120</address>
              <http-method>POST</http-method>
              <protocol>HTTP</protocol>
              <port>9563</port>
            </entry>
          </server>
          <format>
            <threat>
              <name>SFNapi</name>
              <url-format>/sfn-dns</url-format>
              <headers>
                <entry name="Content-Type">
                  <value>application/json</value>
                </entry>
              </headers>
              <payload>{
                "msg_gen_time": "$time_generated",
                "device_name": "$device_name",
                "src_ip": "$src",
                "dst_ip": "$dst",
                "threat_id": "$threatid",
                "threat_id": "$threatid",
                "threat_category": "$thr_category",
                "rule": "$rule"
             }</payload>
            </threat>
          </format>
        </entry>
      </http>
      <profiles>
        <entry name="HTTP log forwarding">
          <match-list>
            <entry name="SFN Log Forwarding">
              <send-http>
                <member>SFN HTTP Server</member>
              </send-http>
              <log-type>threat</log-type>
              <filter>All Logs</filter>
              <send-to-panorama>no</send-to-panorama>
            </entry>
          </match-list>
        </entry>
      </profiles>
    </log-settings>